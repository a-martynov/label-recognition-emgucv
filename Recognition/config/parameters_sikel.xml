<?xml version="1.0" encoding="utf-8" ?>
<!-- Instellingen voor productiehal sikel -->
<parameters>
  <!-- Region of interest: bereik waar rolnummer gezocht moet worden -->
  <roi_x>290</roi_x>
  <roi_y>160</roi_y>
  <roi_width>470</roi_width>
  <roi_height>300</roi_height>
  <!-- Beeldverwerking parameters -->
  <dilate>10</dilate>
  <erode>20</erode>
  <gaussuan_smt>9.5</gaussuan_smt>
  <gaussuan_smt2>11.5</gaussuan_smt2>
  <!-- Rolnummer eigenschappen -->
  <label_width>20</label_width>
  <label_height>15</label_height>
  <label_fault>8</label_fault>
  <!-- SearchTable: Interval tussen punten en maximum frequencie -->
  <highbound_frequency>20</highbound_frequency>
  <max_distance_between_points>300</max_distance_between_points>
  <!-- Tesseract: minimaal en maximaal aantal karakters -->
  <min_char>4</min_char>
  <max_char>7</max_char>
</parameters>